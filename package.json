{
    "name": "routify-starter",
    "private": true,
    "scripts": {
        "dev": "run-p dev:*",
        "dev:routify": "routify",
        "dev:vite": "vite",
        "preview": "vite preview",
        "build": "npm-run-all -p serve build:routify build:vite",
        "build:routify": "routify -b",
        "build:vite": "vite build",
        "serve": "node ./app/index.js",
        "vdev:client": "vercel dev --local-config ./vercel-dev.json",
        "vdev": "npm-run-all -p vdev:client serve"
    },
    "devDependencies": {
        "@roxi/routify": "^2.18.7",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.44",
        "@vercel/node": "^2.4.1",
        "autoprefixer": "^10.4.7",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.14",
        "postcss-import": "^14.1.0",
        "svelte": "^3.48.0",
        "svelte-preprocess": "^4.10.6",
        "vite": "^2.9.9"
    },
    "dependencies": {
        "express": "^4.18.1"
    }
}
